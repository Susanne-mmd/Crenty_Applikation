<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:android1="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/loginpage"
    android:orientation="vertical" >

    <LinearLayout
        android1:layout_width="wrap_content"
        android1:layout_height="match_parent"
        android1:orientation="vertical" >

        <Button
            android1:id="@+id/btnregister"
            android1:layout_width="match_parent"
            android1:layout_height="wrap_content"
            android1:text="Registrer" />
        
        <Button
            android1:id="@+id/btnglemtkodeord"
            android1:layout_width="318dp"
            android1:layout_height="wrap_content"
            android1:text="Glemt kodeord" />

        <Button
            android1:id="@+id/btnskiftkodeord"
            android1:layout_width="match_parent"
            android1:layout_height="wrap_content"
            android1:text="Skift kodeord" />

        <Button
            android1:id="@+id/btnback"
            android1:layout_width="match_parent"
            android1:layout_height="wrap_content"
            android1:text="Tilbage til login" />


    </LinearLayout>

</RelativeLayout>
